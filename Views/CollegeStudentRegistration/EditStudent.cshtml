@model UAAAS.Models.StudentRegistration

@{
    ViewBag.Title = "EditStudent";
}

<h2>Edit Student Registration</h2>


<link href="@Url.Content("~/Content/jntuh-styles.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.base.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.button.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/themes/base/jquery.ui.theme.css")" rel="stylesheet" type="text/css" />



<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Validations.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jspdf.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jspdf.plugin.from_html.js")" type="text/javascript"></script>
<link href="@Url.Content("~/Content/themes/base/jquery.ui.datepicker.css")" rel="stylesheet" type="text/css" />

@using (Ajax.BeginForm("EditStudent", "CollegeStudentRegistration", new AjaxOptions(), new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model=>model.Id)
    @Html.HiddenFor(model=>model.AadhaarNumber)
    <fieldset>
        <div class="editor-label" style="width:  200px;">
            NAME

        </div>
        <div class="editor-field" style="width: 280px;">
            @Html.DisplayFor(model => model.Name)
        </div>
        <br />
        <div class="editor-label" style="width: 200px;">
            Hall Ticket/Roll Number

        </div>
        <div class="editor-field" style="width: 280px;">
            @Html.EditorFor(model => model.UpdateHallTicketNo)
            @Html.ValidationMessageFor(model => model.UpdateHallTicketNo)
        </div>
        <div style="text-align: center">
            <p>
                <input type="submit" value="Submit" />
            </p>
        </div>
    </fieldset>
}

