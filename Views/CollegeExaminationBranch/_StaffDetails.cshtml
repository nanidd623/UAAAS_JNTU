@model UAAAS.Models.CollegeExaminationBranchstaffDetails
<div class="details_table">

    <div class="display-label" style="width:200px">Name of the Staff</div>
    <div class="display-field" style="width:350px">
        @Html.DisplayFor(model => model.staffName)
    </div>

    <div class="display-label" style="width:200px">Designation</div>
    <div class="display-field" style="width:350px">
        @Html.DisplayFor(model => model.staffDesignation)
    </div>

    <div class="display-label" style="width:200px">Teaching/Non-Teaching</div>
    <div class="display-field" style="width:350px">
        @if (this.Model.isTeachingStaff == true)
        { <label>Teaching</label>}
        else
        {<label>Non-Teaching</label>}
    </div>

</div>
