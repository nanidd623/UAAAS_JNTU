@model UAAAS.Models.CollegePlacement
@{
    Layout = "~/Views/Shared/_LayoutLeftMenu.cshtml";
    if (Roles.IsUserInRole("Admin") || Roles.IsUserInRole("DataEntry"))
    {
        Layout = "~/Views/Shared/_DataEntryLayoutLeftMenu.cshtml";
    } 
    ViewBag.Title = "College Placement Details";
}

<h2>College Placement Details</h2>


<table class="webGrid">
    @*@if (ViewBag.IsUpdate == true)
        {
            @Html.HiddenFor(model => model.id)
        }*@
    <tr>
        <td class="col1">
            @Html.LabelFor(model => model.degreeID)
        </td>

        <td class="col2">
            @Html.HiddenFor(model => model.degreeID)
            @Html.DisplayFor(model => model.degree)
        </td>
    </tr>
    <tr>
