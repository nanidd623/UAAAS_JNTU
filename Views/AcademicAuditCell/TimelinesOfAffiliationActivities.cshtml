
@{
    ViewBag.Title = "Timelines Of Affiliation Activities";
    Layout = "~/Views/Shared/_AcademicAuditCellLayoutPage1.cshtml";
}

<!DOCTYPE html>
<html>
<body>
    <h2>Timelines Of Affiliation Activities @ViewBag.AcademicYear</h2>
    <br />

    <table class="webGrid" style="width:100%;margin:auto">
        <thead class="col1">
            <tr>
                <th style="width: 50px; text-align: center;">
                    S.No
                </th>
                <th style="text-align: left;">
                    Activity
                </th>
                <th style="text-align: left;">
                    Month
                </th>
            </tr>
        </thead>
        <tbody class="col2">
                    <tr>
                        <td style="width: 50px; text-align: center;">
                            1
                        </td>
                        <td style="text-align: left;">
                            Issue of circular for applying for NOC for closure of course / Closure of Institution.
                        </td>
                        <td style="text-align: left;">
                            December
                        </td>
                    </tr>
      
        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    2
                </td>
                <td style="text-align: left;">
                    Meeting with Principals/Managements
                </td>
                <td style="text-align: left;">
                    December
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    3
                </td>
                <td style="text-align: left;">
                    Faculty Selection process initiation ensuing Academic Year
                </td>
                <td style="text-align: left;">
                    December
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    4
                </td>
                <td style="text-align: left;">
                    Issue of notification for online application for affiliation for ensuing Academic Year.
                </td>
                <td style="text-align: left;">
                    January
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    5
                </td>
                <td style="text-align: left;">
                    Schedule of Inspections to colleges
                </td>
                <td style="text-align: left;">
                    March
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    6
                </td>
                <td style="text-align: left;">
                    Evaluation of FFC Reports by SCA team
                </td>
                <td style="text-align: left;">
                    April
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    7
                </td>
                <td style="text-align: left;">
                    Generation of Deficiency Reports and communication to the colleges
                </td>
                <td style="text-align: left;">
                    April
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    8
                </td>
                <td style="text-align: left;">
                    Appeal by colleges
                </td>
                <td style="text-align: left;">
                    May
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    9
                </td>
                <td style="text-align: left;">
                    Re-Inspections if any
                </td>
                <td style="text-align: left;">
                    May
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    10
                </td>
                <td style="text-align: left;">
                    Evaluation of Re-inspection Reports
                </td>
                <td style="text-align: left;">
                    May
                </td>
            </tr>

        </tbody>
        <tbody class="col2">
            <tr>
                <td style="width: 50px; text-align: center;">
                    11
                </td>
                <td style="text-align: left;">
                    Generation of final affiliation letters
                </td>
                <td style="text-align: left;">
                    May
                </td>
            </tr>

