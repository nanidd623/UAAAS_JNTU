@model List<UAAAS.Controllers.BASFlagDeficiencyFacultyController.CollegeWiseBasFlagFaculty>
@{
    ViewBag.Title = "ExcelFileDownload";
    int Count = 1;
}
@using (Html.BeginForm())
{
    int data = 0;
    <table style="width: 100%; border: 1px;" border="1">
        <tr>
            <th style="text-align: center; vertical-align: top; background-color:yellow">SNo</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">CollegeCode</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">CollegeName</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">RegistrationNumber</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">BAS Joining Date</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">July</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">August</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">September</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">October</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">November</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">December</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">January</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">February</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">March</th>
            <th style="text-align: center; vertical-align: top; background-color: yellow">Status</th>

        </tr>




        @if (Model != null)
        {
            foreach (var item in Model)
            {
                <tr>
                    <td style="text-align: left; vertical-align: top;">@Count</td>

                    <td style="text-align: left; vertical-align: top;">@item.CollegeCode</td>
                    <td style="text-align: left; vertical-align: top;">@item.CollegeName</td>
                    <td style="text-align: left; vertical-align: top;">@item.Registraionnumber</td>
                    <td style="text-align: left; vertical-align: top;">@item.BASJoiningDate</td>
                    <td style="text-align: left; vertical-align: top;">@item.July</td>
                    <td style="text-align: left; vertical-align: top;">@item.August</td>
                    <td style="text-align: left; vertical-align: top;">@item.September</td>
                    <td style="text-align: left; vertical-align: top;">@item.October</td>
                    <td style="text-align: left; vertical-align: top;">@item.November</td>
                    <td style="text-align: left; vertical-align: top;">@item.December</td>
                    <td style="text-align: left; vertical-align: top;">@item.January</td>
                    <td style="text-align: left; vertical-align: top;">@item.February</td>
                    <td style="text-align: left; vertical-align: top;">@item.March</td>
                    <td style="text-align: left; vertical-align: top;">@item.Cleared</td>
                </tr>
                Count++;
            }
        }
       

    </table>

 
}