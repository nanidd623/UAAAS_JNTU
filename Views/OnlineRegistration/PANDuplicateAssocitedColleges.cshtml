@model IEnumerable<UAAAS.Models.FacultyRegistration>
@{
    ViewBag.Title = "PAN Number Duplicates";

    var index = 1;
    UAAAS.Models.uaaasDBContext db = new UAAAS.Models.uaaasDBContext();
    var jntuh_college=db.jntuh_college.ToList();
    string CollegeCode = string.Empty;
    
}

<h2>PAN Number Duplicates</h2>
@if (TempData["Error"] != null)
{
    <div class="error">
        <p><strong>Error:</strong> @TempData["Error"].ToString()</p>
    </div>
}
@if (TempData["Success"] != null)
{
    <div class="success">
        <p><strong>Success:</strong> @TempData["Success"].ToString()</p>
    </div>
}
<br/>

<script src="~/Scripts/jquery.dataTables.min.js"></script>



<table id="example" class="display" width="100%">
    <thead>
        <tr>
            <th style="width: 60px; text-align: center;">
                S.No
            </th>
            <th style="width: 150px; text-align: center;">
                Registration ID
            </th>
            <th style="width: 120px; text-align: center;">
                Type
            </th>
            <th style="width: 60px; text-align: center;">
                Code
            </th>
            <th style="text-align: left;">
                Full Name
            </th>
            <th style="width: 120px; text-align: center;">
                Mobile
            </th>
            <th style="width: 120px; text-align: center;">
                PAN Number
            </th>
            <th style="width: 140px; text-align: center;">
                Aadhaar Number
            </th>
            <th  style="width: 80px; text-align: center;">
                Absent
            </th>
            <th  style="width: 80px; text-align: center;">
                BlackList
            </th>
            <th class="ui-state-default" style="width: 80px; text-align: center;">
                Photo
            </th>
            <th class="ui-state-default" style="width: 50px;"></th>
            <th class="ui-state-default" style="width: 50px;"></th>
            <th class="ui-state-default" style="width: 50px;"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.OrderBy(e=>e.PANNumber).ToList())
        {
            if (@item.CollegeId != null)
            {

                CollegeCode = jntuh_college.Where(e => e.id == @item.CollegeId).Select(e => e.collegeCode).FirstOrDefault();
            }

            <tr>
                <td style="text-align: center; vertical-align: middle;">
                        @(index++)
            </td>
            <td style="text-align: center; text-transform: uppercase; vertical-align: middle;">
                @Html.DisplayFor(modelItem => item.RegistrationNumber)
            </td>
            <td style="text-align: center; text-transform: uppercase; vertical-align: middle;">
                @if (item.Type.ToUpper() == "EXISTFACULTY")
                { <span style="color: green;">Existing</span>}
                else
                { <span style="color: red;">New</span>}
            </td>
            <td style="text-align: center; text-transform: uppercase; vertical-align: middle;">
                    @CollegeCode
                </td>
            <td style="vertical-align: middle; text-transform: uppercase;">
                @Html.DisplayFor(modelItem => item.FirstName)
                @Html.DisplayFor(modelItem => item.MiddleName)
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td style="text-align: center; vertical-align: middle;">
                @Html.DisplayFor(modelItem => item.Mobile)
            </td>

           
            <td style="text-align: center; vertical-align: middle;">
                @Html.DisplayFor(modelItem => item.PANNumber)
            </td>

                <td style="text-align: center; vertical-align: middle;">
                    @Html.DisplayFor(modelItem => item.AadhaarNumber)
                </td>
                <td style="text-align: center; vertical-align: middle;">
                @if (item.Absent != false && item.Absent!=null)
                {
                    <span style="color: red">Yes</span>
                }


            </td>
            <td style="text-align: center; vertical-align: middle;">
                @if (item.BlacklistFaculty !=false && item.BlacklistFaculty!=null)
                {
                    <span style="color:red">Yes</span>
                }
                

            </td>


           
                <td style="text-align: center; vertical-align: middle;">
                    @if (@item.facultyPhoto != null)
                    {
                        <img src="../Content/Upload/Faculty/Photos/@item.facultyPhoto.Replace("~", "")" style="height: 50px;"/>
                    }
                    else
                    {
                        <img src="../Content/Images/no-photo.gif" style="height: 50px;"/>
                    }
                </td>

                <td style="text-align: center; vertical-align: middle;">
                    @if (item.BlacklistFaculty == false || item.BlacklistFaculty == null)
                    {
                          @Html.ActionLink("View", "FacultyVerification", "FacultyVerificationDENew", new {fid = UAAAS.Models.Utilities.EncryptString(item.id.ToString(), System.Web.Configuration.WebConfigurationManager.AppSettings["CryptoKey"])}, new {@class = "edit_button1", target = "_blank"})
                    }
                   
                </td>
                <td style="text-align: center; vertical-align: middle;">

                    @if (item.BlacklistFaculty == false || item.BlacklistFaculty == null)
                    {
                         @Html.ActionLink("Hide", "PANHide", "OnlineRegistration", new { fid = UAAAS.Models.Utilities.EncryptString(item.id.ToString(), System.Web.Configuration.WebConfigurationManager.AppSettings["CryptoKey"]) }, new { @class = "create_button" })
                        
                    }

                </td>
                <td style="text-align: center; vertical-align: middle;">

                    @if (item.BlacklistFaculty == false || item.BlacklistFaculty == null)
                    {
                        @Html.ActionLink("Not Clear", "NotApproved", "OnlineRegistration", new { fid = UAAAS.Models.Utilities.EncryptString(item.id.ToString(), System.Web.Configuration.WebConfigurationManager.AppSettings["CryptoKey"]) }, new { @class = "create_button" })
                    }

                </td>

        </tr>
        }
    </tbody>

    <tfoot>
        <tr>
            <th>
                <input type="text" id="i-0" style="width: 80%; text-align: left; display: none;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-1" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-2" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-3" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-4" style="width: 90%; text-align: left" class="search_init">
            </th>
            <th>
                <input type="text" id="i-5" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-6" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-7" style="width: 90%; text-align: center;" class="search_init">
            </th>
            <th>
                <input type="text" id="i-10" style="width: 90%; text-align: left; " class="search_init">
            </th>
            <th>
                <input type="text" id="i-11" style="width: 90%; text-align: left; " class="search_init">
            </th>
            <th class=" ui-state-default">
                <input type="text" id="i-8" style="width: 90%; text-align: left; display: none;" class="search_init">
            </th>

            <th class=" ui-state-default">
                <input type="text" id="i-9" style="width: 90%; text-align: left; display: none;" >
            </th>
            <th class=" ui-state-default">
                <input type="text" id="i-12" style="width: 90%; text-align: left; display: none;">
            </th>
            <th class=" ui-state-default">
                <input type="text" id="i-13" style="width: 90%; text-align: left; display: none;">
            </th>
        </tr>
    </tfoot>
</table>
