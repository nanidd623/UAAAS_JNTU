    <tbody>
        @if (Model != null)
        {
            foreach (var item in Model.OrderBy(a => a.CollegeName).ToList())
            {
                <tr>
                    <td>@(Count++)</td>
                    <td>@item.CollegeCode</td>
                    <td>@item.CollegeName</td>
                    @if (item.ShiftId == 2)
                    {
                        <td>@item.Degree - @item.Specialization -@item.ShiftId</td>
                    }
                    else
                    {
                        <td>@item.Degree - @item.Specialization </td>
                    }

                    @if (PresentYear == true)
                    {
                        <td>@item.ProposedIntake</td>
                        <td>@item.PresentYearAICTEApproved</td>
                        <td>@item.PresentYearJntuApproved</td>
                        <td>@item.PresentYearExamsBranchRegularAdmitted</td>
                        <td>@item.PresentYearExamsBranchLateralAdmitted</td>
                        <td>@item.PresentYearCollegeAdmitted</td>
                    }

                    @if (FirstYear == true)
                    {
