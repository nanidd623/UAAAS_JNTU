                    <th style="text-align: center; background-color: yellow; vertical-align: top">
                        AAP @ViewBag.FirstYear
                    </th>
                }
                if (JntuApproved == true)
                {
                    <th style="text-align: center; background-color: yellow; vertical-align: top">
                        JAP @ViewBag.FirstYear
                    </th>
                }
                if (ExamBranchAdmittedR == true)
                {
                    <th style="text-align: center; background-color: yellow; vertical-align: top">
                        EADR @ViewBag.FirstYear
                    </th>
                }
                if (ExamBranchAdmittedL == true)
