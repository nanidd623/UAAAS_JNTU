    var FirstYear = Model.Select(a => a.FirstYear).FirstOrDefault();
    var SecondYear = Model.Select(a => a.SecondYear).FirstOrDefault();
    var ThirdYear = Model.Select(a => a.ThirdYear).FirstOrDefault();
    var FourYear = Model.Select(a => a.FourYear).FirstOrDefault();
    var FifthYear = Model.Select(a => a.FifthYear).FirstOrDefault();
    var SixYear = Model.Select(a => a.SixYear).FirstOrDefault();

    var AICTEApproved = Model.Select(a => a.AICTEApprovedIntake).FirstOrDefault();
    var JntuApproved = Model.Select(a => a.JntuApprovedIntake).FirstOrDefault();
    var ExamBranchAdmittedR = Model.Select(a => a.ExamsAdmittedIntakeR).FirstOrDefault();
    var ExamBranchAdmittedL = Model.Select(a => a.ExamsAdmittedIntakeL).FirstOrDefault();
