@model UAAAS.Models.CompletedSubmission
@{

}
@using (Html.BeginForm())
{
    int count = 0;
    <table border="1" style="width: 100%">
        <thead>
            <tr>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    S.No
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Code
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Name
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    District
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Type
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Submitted Date
                </th>

            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Colleges)
            {
                <tr>
                    <td style="text-align: center;vertical-align: top">
                        @(count = count + 1)
                    </td>
                    <th style="text-align: center;vertical-align: top">
                        @item.collegeCode
                    </th>
                    <th style="text-align: left; vertical-align: top">
                        @item.collegeName
                    </th>
                    <th style="text-align: left; vertical-align: top">
                        @item.district
                    </th>
                    <th style="text-align: left; vertical-align: top">
                        @item.Collegetype
                    </th>
                    <th style="text-align: left; vertical-align: top">
                        @item.submitdate
                    </th>

                </tr>
            }
        </tbody>
    </table>
}
