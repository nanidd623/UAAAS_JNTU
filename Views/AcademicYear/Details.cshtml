@model UAAAS.Models.jntuh_academic_year

@{
    ViewBag.Title = "Academic Year Details";
}

<h2>Academic Year Details</h2>

<div class="details_table">

    <div class="display-label">Academic Year</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.academicYear)
    </div>

    <div class="display-label">Actual Year</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.actualYear)
    </div>

    <div class="display-label">Is Present Academic Year</div>
    <div class="display-field">
        @*@Html.DisplayFor(model => model.isPresentAcademicYear)*@
         @if (this.Model.isPresentAcademicYear == true)
        { <label>Yes</label>}
        else
        {<label>No</label>}
    </div>

    <div class="display-label">Status</div>
    <div class="display-field">
        @*@Html.DisplayFor(model => model.isActive)*@
        @if (this.Model.isActive == true)
        { <label>Active</label>}
        else
        {<label>Inactive</label>}
    </div>

    <div class="display-label">Created Date</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.createdOn)
    </div>

    <div class="display-label">Created By</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.my_aspnet_users.name)
    </div>

    <div class="display-label">Updated Date</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.updatedOn)
    </div>

    <div class="display-label">Updated By</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.my_aspnet_users1.name)
    </div>
     <div class="display-label last-row">
        &nbsp;
    </div>
    <div class="display-field last-row">
        <p>
            @Html.ActionLink("Back", "Index", null, new { @class = "back_button" })
            @Html.ActionLink("Edit", "Edit", new { id = Model.id }, new { @class = "submit_button" })
        </p>
    </div>
    <div class="clear"></div>
</div>
