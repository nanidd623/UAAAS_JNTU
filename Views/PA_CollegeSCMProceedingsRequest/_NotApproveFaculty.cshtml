@model UAAAS.Controllers.College.Facultynotapproved

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Ajax.BeginForm("NotApproveFaculty", "PA_CollegeSCMProceedingsRequest", new AjaxOptions()))
{
    @Html.HiddenFor(model=>model.FacultyAddId)
    @Html.HiddenFor(model=>model.CollegeId)
    <div style="text-align: center; background-color: #FCF1D1">
        <p style="text-align: left"><strong>De-Activation Reason</strong></p>
        @Html.TextAreaFor(model => model.DeactivationReason, new {@required=true,@class="required", @style = "resize: none;width: 300px;height: 50px"})
        @Html.ValidationMessageFor(model => model.DeactivationReason)
    </div>
    <p style="text-align: center"> <input type="submit" value="Submit" /></p>
}
