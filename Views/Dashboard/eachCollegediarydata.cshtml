                            @Html.DisplayFor(model => model.PrincipalMobile, new { @class = "txtbox" })
                        </div>
                    </td>
                    <td>
                        <div class="editor-field" style="width: 100px;">
                            @Html.DisplayFor(model => model.PrincipalMobile, new { @class = "txtbox" })
                        </div>
                    </td>
                    <td>
                        <div class="editor-field" style="width: 250px;">
                            @Html.DisplayFor(model => model.PrincipalEmail, new { @class = "txtbox" })
                        </div>
                    </td>
                </tr>
            </table>
