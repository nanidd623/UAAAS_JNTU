    <script src="//cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.min.js"></script>

    <script type="text/javascript">
        
        $(document).ready(function () {
           
            var FruitsData = @Html.Raw(Json.Encode(data123));
       
