@{
    ViewBag.Title = "View1";
    // Layout = null;
    List<SelectListItem> data123 = new List<SelectListItem>()
            {
                  new SelectListItem(){Value = "10",Text = "Apple"},
                  new SelectListItem(){Value = "15",Text = "Orange"},
                  new SelectListItem(){Value = "25",Text = "Graphs"},
                  new SelectListItem(){Value = "30",Text = "Bannana"},
                  new SelectListItem(){Value = "25",Text = "Others"},
            };
}
<!DOCTYPE html>
<html>
<head>

    <title></title>
