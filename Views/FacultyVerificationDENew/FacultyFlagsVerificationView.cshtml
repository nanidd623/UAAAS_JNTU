                    {
                        Phd = 1;
                    }
                    else if (@item.Phd2pages)
                    {
                        Phd = 1;
                    }
                }

                //if (item.Absent == true || item.NoSCM == true || item.VerificationStatus == true || item.OriginalCertificatesnotshownFlag == true || item.NOrelevantUgFlag == true || item.NOrelevantPgFlag == true || item.NOrelevantPhdFlag == true || item.InvalidPANNo == true || item.Type == "Adjunct" || item.BlacklistFaculty == true || item.DegreeId < 4 || item.NOTQualifiedAsPerAICTE == true || item.NoClass == true)
                if (item.Absent == true || item.NoSCM == true || item.VerificationStatus == true || item.Type == "Adjunct" || item.BlacklistFaculty == true || item.DegreeId < 4 || item.NoClass == true)
                {
                    color = "red";
                }
                else
                {
                    color = string.Empty;
                }
