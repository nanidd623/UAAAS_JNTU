                            <span style="color: red;">Yes</span>
                        }
                    </td>
                    <td style="text-align: center; vertical-align: middle;">
                        @if (item.NOrelevantPhdFlag == true)
                        {
                            <span style="color: red;">Yes</span>
                        }
                    </td>
                    @*<td style="text-align: center; vertical-align: middle;">
                            @if (item.NotIdentityFiedForAnyProgramFlag == true)
                            {
                                <span style="color: red;">Yes</span>
                            }
                        </td>*@
                    <td style="text-align: center; vertical-align: middle;">
