    $(document).ready(function () {

        $(".approve_link").click(function () {
            if (confirm("Are you sure, you want to approve the faculty?")) {
                return true;
            }
            else {
                return false;
            }
        });

        $(".Terms").click(function () {
            if ($(this).val() == "True") {
                $(".submitButton").show();
                $(".rbtn").css("background-color", "#E8EFF3");
            }
            else {
                $(".submitButton").hide();
            }
        });

        var $myDialog = $("#dialog-edit11").dialog({
            title: 'Add/Edit Department',
            autoOpen: false,
            resizable: false,
            height: 200,
            width: 600,
            show: { effect: 'drop', direction: "up" },
            modal: true,
            draggable: true,
            closeOnEscape: true,
            open: function (event, ui) {
                $(this).load(url);
            },
            close: function (event, ui) {
                $(this).dialog("close");
            }
        });

        $("#openDialog").live("click", function (e) {
            e.preventDefault();
            $("#dialog-edit11").dialog({
                title: 'Not Approved Details'
            });
            url = $("#openDialog").attr('href');

            $myDialog.dialog('open');
            return false;
        });

        $("#Dialog_Approved").live("click", function (e) {

            var url = $(this).attr('href');

            $("#Approvedflags").dialog({
                title: 'Approved Flags',
                autoOpen: false,
                resizable: false,
                height: 170,
                width: 350,
                show: { effect: 'drop', direction: "up" },
                modal: true,
                draggable: true,
                buttons: {
                    "Yes": function () {
                        $(this).dialog("close");
                        window.location = url;

                    },
                    "No": function () {
                        $(this).dialog("close");

                    }
                }
            });

            $("#Approvedflags").dialog('open');
            return false;
        });

        $("#Dialog_NotApproved").click(function () {
            var url = $(this).attr('href');

            $("#NotApprovedflags").dialog({
                title: 'Not-Approved Flags',
                width: 750,
                height: 300,
                model: true,
                resizable: false,
                autoOpen: false,
                show: { effect: 'drop', direction: 'up' },
                draggable: true,
                open: function (event, ui) {
                    $(this).load(url);
