                    //$(this).dialog("close");
                    location.reload();
                    // window.Location = '/FacultyVerificationIndex/ViewFacultyDetails?fid='+ @UAAAS.Models.Utilities.EncryptString(Model.id.ToString(), System.Web.Configuration.WebConfigurationManager.AppSettings["CryptoKey"]);
                }


