@model IEnumerable<UAAAS.Models.registeredfacultylist>
@using (Html.BeginForm())
{

    <table border="1" style="width: 100%">
        <tr>
            <th style="text-align: center; vertical-align: top; background-color: yellow;">
                College Code
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                College Name
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                Registration Number
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                Faculty Name
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                Identifier For
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                Department
            </th>
            <th style="text-align: left; vertical-align: top; background-color: yellow;">
                Specialization
            </th>

        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align: left; vertical-align: top;">
                    <label style="text-transform: uppercase">@item.Collegecode</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.collegename</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.RegistrationNumber</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.FacultyName</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.identifierfor</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.departmentName</label>
                </td>
                <td style="text-align: left; vertical-align: top;">
                    <label>@item.specialization</label>
                </td>

            </tr>
        }

    </table>
}