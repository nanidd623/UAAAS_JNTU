@model UAAAS.Models.AdminLand

@{
    ViewBag.Title = "CollegeFreeSpace";
}

<h2>Avaliable Open Land</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@if (TempData["Success"] != null)
{
    <div class="success">
        <p><strong>Success:</strong> @TempData["Success"].ToString()</p>
    </div>
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <table class="webGrid" style="width: 100%;">

        <tr>
            <td style="text-align: left;" class="col1">
                <span>&nbsp;&nbsp;&nbsp;Avaliable Open Land Area (In Acres) : </span>
            </td>
            <td class="col2">
                @if (@Model.availableArea != null)
                {
                    @Html.DisplayFor(model => model.availableArea)

                }
                else
                {
                    @Html.EditorFor(model => model.availableArea)
                    @Html.ValidationMessageFor(model => model.availableArea)

                    <span>&nbsp;&nbsp;&nbsp;</span><input type="submit" value="Submit" onclick="return validate();" style="height:21px;" />
                }


            </td>
        </tr>
    </table>

}

