@model IEnumerable<UAAAS.Models.SocietyInformation>
@{
    ViewBag.Title = "RTI_AllSocietiesInformation";
    var index = 1;
}
@using (Html.BeginForm())
{
    int count = 0;
    <table border="1" style="width: 100%">
        <thead>
            <tr>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Sl.No
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Code
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Name
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Year of Establishment
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Registered Number
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Society Name
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Address
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Town (or) City</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Mandal</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">State</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">District</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Pincode</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Fax</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Landline</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Mobile</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Email</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Website Address</th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Year of Establishment of the Institution
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Date on which first approval was accorded by the AICTE<span>(DD/MM/YYYY)</span>
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Date on which first affiliation was accorded by the JNTU/JNTUH<span>(DD/MM/YYYY)</span>
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">Year of commencement of First Batch</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="text-align: center;vertical-align: top">
                        @(index++)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.collegeCode)
                    </td>
                    <td style="text-align: left; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.collegeName)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.societyEstablishmentYear, new { Value = item.societyEstablishmentYear == 0 ? "" : item.societyEstablishmentYear.ToString(), @maxlength = "4" })
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.societyRegisterNumber)
                    </td>
                    <td style="text-align: left; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.societyName)
                    </td>
                    <td style="text-align: left; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.address)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.townOrCity)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.mandal)
                    </td>
                    <td style="text-align: center; vertical-align: top"> 
                        @Html.DisplayFor(modelItem => item.stateName)
                    </td>
                    <td style="text-align: center; vertical-align: top"> 
                        @Html.DisplayFor(modelItem => item.districtName)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.pincode, new { Value = item.pincode == 0 ? "" : item.pincode.ToString() })
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.fax)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.landline)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.mobile)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.email)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.website)
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.instituteEstablishedYear, new { @class = "year", Value = item.instituteEstablishedYear == 0 ? "" : item.instituteEstablishedYear.ToString(), @maxlength = "4" })
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.firstApprovalDateByAICTE, new { @class = "calendar1", style = "width: 80px", @onblur = "IsValidDate(this,event.keyCode,0,0,1)", @onkeydown = "return IsValidFormat(this,event.keyCode)" })
                    </td>
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.firstAffiliationDateByJNTU, new { @class = "calendar1", style = "width: 80px", @onblur = "IsValidDate(this,event.keyCode,0,0,1)", @onkeydown = "return IsValidFormat(this,event.keyCode)" })
                    </td> 
                    <td style="text-align: center; vertical-align: top">
                        @Html.DisplayFor(modelItem => item.firstBatchCommencementYear, new { @class = "year", Value = item.firstBatchCommencementYear == 0 ? "" : item.firstBatchCommencementYear.ToString(), @maxlength = "4" })
                        @Html.HiddenFor(modelItem => item.addressTye, new { Value = "SOCIETY" })
                        @Html.HiddenFor(modelItem => item.collegeId)
                        @Html.HiddenFor(modelItem => item.createdOn, new { Value = DateTime.Now })
                    </td> 
                </tr>  
            }
        </tbody>
    </table>
}

