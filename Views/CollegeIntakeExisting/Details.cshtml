<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

<style type="text/css">
    .width {
        width: 80%;
    }

    .text_width {
        width: 28%;
        text-align: center;
    }
    .left_width {
        width: 300px;
    }

    .right_width {
        width: 325px;
    }

    .left_width_down {
        width: 300px;
