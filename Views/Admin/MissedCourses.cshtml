@model UAAAS.Models.MissedCourses
@{
    ViewBag.Title = "MissedCourses";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>MissedCourses</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
@if (TempData["Status"] != null)
{
    <div class="success">
        @TempData["Status"]
    </div>
}
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
   

    <div class="editor-label">
        <span>College Code</span>
    </div>
    <div class="editor-field" style="width:300px">
        @Html.DropDownListFor(model => model.collegeID,
        new SelectList(ViewBag.collegeCodes as System.Collections.IEnumerable, "id", "collegeCode"),
        "--Select--", new { id = "collegeCode", @style = "width:165px" })
        @Html.ValidationMessageFor(model => model.collegeID)
    </div>
    <p>
        <input type="submit" value="Create" style="margin-left: 10px;height: 33px;margin-top: 2px;"/>
    </p>
}

