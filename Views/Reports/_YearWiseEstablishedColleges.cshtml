@{
    int sno = 1;
}
<table border="1" style="width: 100%">
    <tr>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">SNo</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Year</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Engineering</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Pharmacy</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Standalone</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Integrated Campus</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Technical Campus</th>
        <th style="text-align: center; background-color: yellow; vertical-align: top;">Total</th>
     
     
    </tr>
    @foreach (var item in ViewBag.yearWiseEstablishedCollegesList)
    {
        <tr>
            <th style="text-align: center; vertical-align: top;">@(sno++)</th>
            <td style="text-align: center; vertical-align: top;">@item.year</td>
            <td style="text-align: right; vertical-align: top;">@item.EngineeringCount</td>
            <td style="text-align: right; vertical-align: top;">@item.PharmacyCount</td>
            <td style="text-align: right; vertical-align: top;">@item.StandaloneCount</td>
             <td style="text-align: right; vertical-align: top;">@item.IntegratedCampusCount</td>
            <td style="text-align: right; vertical-align: top;">@item.TechnicalCampusCount</td>
            <td style="text-align: right; vertical-align: top;">@item.totalCount</td>
        </tr>
    }
</table>
