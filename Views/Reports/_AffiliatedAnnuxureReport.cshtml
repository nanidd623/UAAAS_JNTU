@model List<UAAAS.Controllers.Reports.AffiliatedAnnuxureDetailsController.AffilicationClass>
@{
    int count = 0;
}
@using (Html.BeginForm())
{
    <table border="1" width="100%">
        <thead>
            <tr>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Sl.No
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Code
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    College Name
                </th>

                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Degree
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Department
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Specilization
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Proposed Intake
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Required Faculty
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Available Faculty
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Faculty Shortage
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                    Deficiency of Doctorates
                </th>
                <th style="text-align: center; background-color: yellow; vertical-align: top">
                   Labs Shortage
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td style="text-align: center;vertical-align: top">
                        @(count = count + 1)
                    </td>
                    <th style="text-align: center; vertical-align: top">
                        '@item.CollegeCode
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.CollegeName
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.Degree
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.Department
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.Specialization
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.ProposedIntake
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.Required
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.Available
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.TotalIntake
                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.PhdDeficiency

                    </th>
                    <th style="text-align: center; vertical-align: top">
                        @item.LabsDeficiency
                    </th>

                </tr>
            }

        </tbody>
    </table>
}