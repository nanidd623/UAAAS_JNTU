@model UAAAS.Controllers.excelreportclass

@{
    ViewBag.Title = "Download_Excel";
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Validations.js")" type="text/javascript"></script>
<h2>Download NOC College Wise</h2>

@using (Html.BeginForm())
{
    <table style="width: 100%">
        <tr>
            <td class="col1"> NOC Type</td>
            <td class="col2">
                @Html.DropDownListFor(m => m.FeeType, new SelectList(ViewBag.Feetype as System.Collections.IEnumerable, "id", "FeeType"), "--Select--", new { @class = "required" })
            </td>
        </tr>
        <tr>
            <td class="col1"></td>
            <td class="col2">
                <input type="submit" value="Export" />
            </td>
        </tr>
    </table>
   
}