@model UAAAS.Controllers.OnlineRegistration.FacultySubjectsTaught
@*@{
    ViewBag.Title = "EditFacultySubjectsTaught";
}*@

<h2>EditFacultySubjectsTaught</h2>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Validations.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jspdf.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jspdf.plugin.from_html.js")" type="text/javascript"></script>
@using (Html.BeginForm())
{
    <table>
        <tr>
            <td class="col1">
                Registration Number
            </td>
            <td class="col2">
                @Html.DisplayFor(model => model.RegistrationNumber)
                @Html.HiddenFor(model=>model.facultyId)
            </td>
        </tr>
        <tr>
            <td class="col1">
                Subjects Taught
            </td>
            <td class="col2">
                @Html.TextBoxFor(model => model.facultySubjectsTaught, new {@style="width:300px;" ,@class ="required" })
            </td>
        </tr>
        <tr>
            <td class="col1">
                No.of times subjects taught
            </td>
            <td class="col2">
                @Html.TextBoxFor(model => model.NoOfTimesSubjectsTaught, new {@class = "required" })
            </td>
        </tr>
        <tr>
            <td  colspan="2" style="float:right;">
                <input type="submit" value="Edit" />
            </td>
        </tr>
    </table>
}



